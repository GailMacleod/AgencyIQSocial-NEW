# TheAgencyIQ Environment Configuration Template
# Copy this file to .env and fill in your actual values

# Database
DATABASE_URL=postgresql://username:password@localhost:5432/theagencyiq

# Node Environment
NODE_ENV=development
PORT=5000

# Authentication & Security
SUBSCRIPTION_ACTIVE=true
SESSION_SECRET=your-session-secret-here

# X/Twitter API (OAuth 2.0)
X_CONSUMER_KEY=your-x-consumer-key
X_CONSUMER_SECRET=your-x-consumer-secret

# X.AI API (for content generation)
XAI_API_KEY=your-xai-api-key

# Meta/Facebook API
FACEBOOK_APP_ID=your-facebook-app-id
FACEBOOK_APP_SECRET=your-facebook-app-secret
FACEBOOK_PAGE_ACCESS_TOKEN=your-facebook-page-token

# Instagram API
INSTAGRAM_CLIENT_ID=your-instagram-client-id
INSTAGRAM_CLIENT_SECRET=your-instagram-client-secret

# LinkedIn API
LINKEDIN_CLIENT_ID=your-linkedin-client-id
LINKEDIN_CLIENT_SECRET=your-linkedin-client-secret

# YouTube/Google API
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
YOUTUBE_API_KEY=your-youtube-api-key

# Stripe (Payments)
STRIPE_PUBLISHABLE_KEY=your-stripe-publishable-key
STRIPE_SECRET_KEY=your-stripe-secret-key
STRIPE_WEBHOOK_SECRET=your-stripe-webhook-secret

# External Services
REPLICATE_API_TOKEN=your-replicate-token
SENDGRID_API_KEY=your-sendgrid-key
TWILIO_ACCOUNT_SID=your-twilio-sid
TWILIO_AUTH_TOKEN=your-twilio-token
TWILIO_PHONE_NUMBER=your-twilio-phone

# Analytics
VITE_GA_MEASUREMENT_ID=your-google-analytics-id

# Replit (Auto-configured)
REPLIT_DOMAINS=auto-configured-by-replit